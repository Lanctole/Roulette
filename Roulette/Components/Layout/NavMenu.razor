@implements IDisposable

@inject NavigationManager NavigationManager
<div class="top-row ps-3 navbar navbar-dark " style="background-color: #000000;">
    <div class="container-fluid " style="justify-content: center;">
        <Icon Component="RouletteSvg" Theme="outline" Style="margin-right: 10px; animation: loadingCircle 7s infinite linear;" />
        <a class="navbar-brand" href="">Roulette</a>
    </div>
</div>

<input type="checkbox" title="Navigation menu" class="navbar-toggler" />

<div class="nav-scrollable" style="background: linear-gradient(to bottom, black, #394187); " onclick="document.querySelector('.navbar-toggler').click()">
    <nav class="flex-column">
       @*  <div class="nav-item px-3">
            <NavLink class="nav-link" href="" Match="NavLinkMatch.All">
                <span class="bi bi-house-door-fill-nav-menu" aria-hidden="true"></span> Home
            </NavLink>
        </div> *@

        <div class="nav-item px-3">
            <NavLink class="nav-link" href="anime">
                
                <Icon Component="AnimeSvg" Theme="outline" Style="margin-right: 10px;" /> Аниме
            </NavLink>
        </div>
        
        <div class="nav-item px-3">
            <NavLink class="nav-link" href="manga">
                <Icon Component="MangaSvg" Theme="outline" Style="margin-right: 10px;" /> Манга
            </NavLink>
        </div>
        
        <div class="nav-item px-3">
            <NavLink class="nav-link" href="ranobe">
                <Icon Component="RanobeSvg" Theme="outline" Style="margin-right: 10px;" /> Ранобе
            </NavLink>
        </div>
       <AuthorizeView Roles="admin">
           <div class="nav-item px-3">
               <NavLink class="nav-link" href="studios">
                   <span class="bi bi-list-nested-nav-menu" aria-hidden="true"></span> Студии
               </NavLink>
           </div>
        
           <div class="nav-item px-3">
               <NavLink class="nav-link" href="genres">
                   <span class="bi bi-list-nested-nav-menu" aria-hidden="true"></span> Жанры
               </NavLink>
           </div>

            <div class="nav-item px-3">
                <NavLink class="nav-link" href="admin">
                    <span class="bi bi-list-nested-nav-menu" aria-hidden="true"></span> Админ Панель
                </NavLink>
            </div>
       </AuthorizeView>
        
       <AuthorizeView>
           <div class="nav-item px-3">
               <NavLink class="nav-link" href="history">
                    <span class="bi bi-list-nested-nav-menu" aria-hidden="true"></span> История колеса
               </NavLink>
           </div>
       </AuthorizeView>
         
       <AuthorizeView>
           <Authorized>
               <div class="nav-item px-3">
                   <NavLink class="nav-link" href="Account/Manage/Email">
                       <span class="bi bi-person-fill-nav-menu" aria-hidden="true"></span> @context.User.Identity?.Name
                   </NavLink>
               </div>
               <div class="nav-item px-3">
                   <form action="Account/Logout" method="post">
                        <AntiforgeryTokenInput />
                       @* <AntiforgeryToken/> *@
                       <input type="hidden" name="ReturnUrl" value="@currentUrl"/>
                       <button type="submit" class="nav-link">
                           <span class="bi bi-arrow-bar-left-nav-menu" aria-hidden="true"></span> Выйти
                       </button>
                   </form>
               </div>
           </Authorized>
           <NotAuthorized>
               <div class="nav-item px-3">
                   <NavLink class="nav-link" href="Account/Register">
                       <span class="bi bi-person-nav-menu" aria-hidden="true"></span> Регистрация
                   </NavLink>
               </div>
               <div class="nav-item px-3">
                   <NavLink class="nav-link" href="Account/Login">
                       <span class="bi bi-person-badge-nav-menu" aria-hidden="true"></span> Войти
                   </NavLink>
               </div>

              @*  <div class="nav-item px-3">
                    <AntiforgeryTokenInput />
                   <button type="button" class="nav-link" data-bs-toggle="modal" data-bs-target="#loginModal">
                       <span class="bi bi-arrow-bar-left-nav-menu" aria-hidden="true"></span> Войти модально
                   </button>
                   <LoginModal ReturnUrl="@NavigationManager.Uri"/>
               </div> *@
           </NotAuthorized>
       </AuthorizeView>
    </nav>
</div>

@code {
    private string? currentUrl;

    protected override void OnInitialized()
    {
        currentUrl = NavigationManager.ToBaseRelativePath(NavigationManager.Uri);
        NavigationManager.LocationChanged += OnLocationChanged;
    }

    private void OnLocationChanged(object? sender, LocationChangedEventArgs e)
    {
        // currentUrl = NavigationManager.ToBaseRelativePath(e.Location);
        // StateHasChanged();
        if (currentUrl != e.Location) // Ensure we only update if there's a real change
        {
            currentUrl = NavigationManager.ToBaseRelativePath(e.Location);
            InvokeAsync(StateHasChanged); // Use InvokeAsync to ensure StateHasChanged is called safely
        }
    }

    public void Dispose()
    {
        // NavigationManager.LocationChanged -= OnLocationChanged;
        if (NavigationManager != null)
        {
            NavigationManager.LocationChanged -= OnLocationChanged;
        }
    }


