{
  "ConnectionStrings": {
    "DefaultConnection": "Server=YOUR_SERVER;Database=YOUR_DATABASE;Username=YOUR_USERNAME;Password=YOUR_PASSWORD;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Auth": {
    "Scope": "user_rates comments topics",
    "Access": "YOUR_ACCESS_TOKEN",
    "Refresh": "YOUR_REFRESH_TOKEN",
    "Name": "YOUR_NAME",
    "ClientId": "YOUR_CLIENT_ID",
    "ClientSecret": "YOUR_CLIENT_SECRET",
    "UserId": "YOUR_USER_ID"
  },
  "Smtp": {
    "Host": "YOUR_SMTP_HOST",
    "Port": 2525,
    "EnableSsl": false,
    "Username": "YOUR_EMAIL",
    "Password": "YOUR_EMAIL_PASSWORD",
    "FromEmail": "YOUR_FROM_EMAIL"
  },
  "Redis": {
    "Configuration": "YOUR_REDIS_SERVER:6379,abortConnect=false,password=YOUR_REDIS_PASSWORD",
    "InstanceName": "YOUR_REDIS_INSTANCE"
  },
  "Authentication": {
    "Yandex": {
      "ClientId": "YOUR_YANDEX_CLIENT_ID",
      "ClientSecret": "YOUR_YANDEX_CLIENT_SECRET"
    }
  },
  "ApiBaseAddress": "http://localhost:8080/api/",
  "Shikimori": {
    "BaseUrl": "https://shikimori.one"
  }
}
